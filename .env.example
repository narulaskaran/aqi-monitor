# API Keys
GOOGLE_AIR_QUALITY_API_KEY="your_google_air_quality_api_key_here"

# Environment
NODE_ENV="development"

# API Configuration
NEXT_PUBLIC_API_URL="http://localhost:3000"

# Database Configuration (Neon)
# DATABASE_URL is the pooled connection used by Prisma Client at runtime
DATABASE_URL="postgresql://user:password@endpoint-pooler.neon.tech/dbname?sslmode=require"

# DIRECT_URL is the direct connection used by Prisma CLI for migrations
DIRECT_URL="postgresql://user:password@endpoint.neon.tech/dbname?sslmode=require" 