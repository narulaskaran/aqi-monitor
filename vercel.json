{
  "version": 2,
  "installCommand": "cd server && npm install",
  "buildCommand": "cd server && npm install && npm run build",
  "outputDirectory": "server/dist",
  "devCommand": "npm run dev",
  "builds": [
    {
      "src": "server/dist/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/trpc/(.*)",
      "dest": "server/dist/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "server/dist/index.js"
    }
  ]
}
